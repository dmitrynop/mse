<div class="react-root"></div>

<ul>
  <%= for set <- @sets do %>
    <li>
      <%= link to: set_path(@conn, :show, set) do %>
        <i class="ss ss-2x ss-<%= set.mtgio_id |> String.downcase %>"></i>
        <span><%= "[#{set.mtgio_id}] #{set.name}" %></span>
      <% end %>
    </li>
  <% end %>
</ul>
